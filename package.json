{
  "name": "queueable",
  "version": "4.1.1",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/queueable.js",
    "default": "./dist/queueable.modern.mjs",
    "types": "./dist/index.d.ts"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/queueable.js",
  "module": "./dist/queueable.module.js",
  "unpkg": "./dist/queueable.umd.js",
  "description": "Convert push-based streams to pull-based async iterables",
  "keywords": [
    "async",
    "queue",
    "es2018",
    "iteration",
    "async-iterators",
    "esnext",
    "streams"
  ],
  "files": [
    "dist"
  ],
  "author": "slikts <dabas@untu.ms>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slikts/queueable.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "prebuild": "rimraf dist",
    "build": "microbundle --external none",
    "docs": "typedoc src/index.ts",
    "dev": "microbundle watch",
    "test": "jest",
    "test:watch": "jest --watch --runInBand --verbose false",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/babel__core": "^7.1.20",
    "@types/babel__preset-env": "^7.9.2",
    "@types/eslint": "^8.4.10",
    "@types/fast-list": "^1.0.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "babel-jest": "^29.3.1",
    "coveralls": "^3.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "microbundle": "^0.15.1",
    "pinst": "^3.0.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.0.7",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "fast-list": "^1.0.3"
  },
  "packageManager": "yarn@3.3.1"
}
