{
  "name": "queueable",
  "version": "4.1.1",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/queueable.js",
    "default": "./dist/queueable.modern.mjs",
    "types": "./dist/index.d.ts"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/queueable.js",
  "module": "./dist/queueable.module.js",
  "unpkg": "./dist/queueable.umd.js",
  "description": "Convert push-based streams to pull-based async iterables",
  "keywords": [
    "async",
    "queue",
    "es2018",
    "iteration",
    "async-iterators",
    "esnext",
    "streams"
  ],
  "files": [
    "dist"
  ],
  "author": "slikts <dabas@untu.ms>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slikts/queueable.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "prebuild": "rimraf dist",
    "build": "microbundle --external none",
    "docs": "typedoc src/index.ts",
    "dev": "microbundle watch",
    "test": "jest",
    "test:watch": "jest --watch --runInBand --verbose false",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@semantic-release/commit-analyzer": "^11.0.0",
    "@semantic-release/github": "^9.0.3",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.3",
    "@types/babel__core": "^7.20.1",
    "@types/babel__preset-env": "^7.9.2",
    "@types/eslint": "^8.40.1",
    "@types/fast-list": "^1.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.0",
    "@types/semantic-release": "^20.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.5.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^14.0.0",
    "microbundle": "^0.15.1",
    "pinst": "^3.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.5",
    "typedoc": "^0.24.8",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "fast-list": "^1.0.3"
  },
  "packageManager": "yarn@3.3.1",
  "volta": {
    "node": "18.13.0"
  }
}
